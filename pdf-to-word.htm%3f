<script type="text/javascript">
window.onload = setLinkBehaviours;
function formatLink(link)
{
  var res;
  var re = /http:\/\/(.*)/ig;
  res = link.replace(re, "$1");
  re = /\//g;
  return res.replace(re, "_");
}
 
function setLinkBehaviours()
{
    var Links = document.getElementsByTagName( ‘A’ );
    for( var i = 0; i < Links.length; i++ )
    {
        Links[i].onclick = function() {
            if(this.href.indexOf("http")>=0 && this.href.indexOf("quick-pdf.com")>=0)
            {
                urchinTracker("/tracker/" + formatLink(this.href));
                        }
        }
    }
}
